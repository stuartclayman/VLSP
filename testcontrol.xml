<SimOptions>
  <GlobalController>
     <Port>8888</Port>
     <StartLocalControllers>true</StartLocalControllers>
     <Simulation>true</Simulation>
     <ConnectedNetwork>true</ConnectedNetwork>
  </GlobalController>
  <LocalController>
     <Name>localhost</Name>
     <Port>10000</Port>
     <LowPort>11001</LowPort>
     <HighPort>12000</HighPort>
  </LocalController>
  <EventEngine>
     <Name>Probabilistic</Name>
     <EndTime>3600</EndTime>
     <Parameters>probdists.xml</Parameters>
  </EventEngine>
  <RouterOptions>
      routertestoptions.xml
  </RouterOptions>
      <Output>
        <File>out.net</File>
        <When>End</When>
        <Type>Network</Type>
    </Output>
    <Output>
        <File>summary.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Summary</Type>
    </Output>
</SimOptions>
