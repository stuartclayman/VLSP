<IKMSEngine>
  <NodeBirthDist>
    <ProbElement>
      <Type>Exponential</Type> 
      <Weight>1.0</Weight>  
      <Parameter>2</Parameter> 
    </ProbElement>
  </NodeBirthDist>

  <NodeDeathDist>
    <ProbElement>
      <Type>Exponential</Type>
      <Weight>0.7</Weight>
      <Parameter>60</Parameter>
    </ProbElement>

    <ProbElement>
      <Type>LogNormal</Type>
      <Weight>1.0</Weight>
      <Parameter>7.0</Parameter>
      <Parameter>1.5</Parameter>
    </ProbElement>
  </NodeDeathDist>

  <LinkCreateDist>
    <ProbElement>
      <Type>PoissonPlus</Type>
      <Weight>1.0</Weight>
      <Parameter>1.5</Parameter>
      <Parameter>1.0</Parameter>
    </ProbElement>
  </LinkCreateDist>

  <NodeCPULoadDist>
   <ProbElement>
    <Type>Uniform</Type>  <!-- Uniform -->
      <Weight>1.0</Weight>  <!-- 100% Type -->
      <Parameter>0</Parameter>  <!-- one every N seconds --> <!-- was 3 -->
      <Parameter>0</Parameter> <!-- was 6 -->
    </ProbElement>
  </NodeCPULoadDist>

  <NodeMemoryLoadDist>
   <ProbElement>
    <Type>Uniform</Type>  <!-- Uniform -->
      <Weight>1.0</Weight>  <!-- 100% Type -->
      <Parameter>0</Parameter>  <!-- one every N seconds --> <!-- was 3 -->
      <Parameter>0</Parameter> <!-- was 6 -->
    </ProbElement>
  </NodeMemoryLoadDist>

  <NodeNetworkLoadDist>
   <ProbElement>
    <Type>Uniform</Type>  <!-- Uniform -->
      <Weight>1.0</Weight>  <!-- 100% Type -->
      <Parameter>1</Parameter>  <!-- one every N seconds --> <!-- was 3 -->
      <Parameter>1</Parameter> <!-- was 6 -->
    </ProbElement>
  </NodeNetworkLoadDist>

  <Parameters>
    <PreferentialAttachment>false</PreferentialAttachment>
    <InitialNumberOfNodes>30</InitialNumberOfNodes>
    <StaticTopology>false</StaticTopology>
  </Parameters>

</IKMSEngine>
