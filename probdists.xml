<ProbabilisticEngine>
      <NodeBirthDist>
            <ProbElement>
                 <Type>Exponential</Type>
                 <Weight>1.0</Weight>
                 <Parameter>10.0</Parameter>
            </ProbElement>
      </NodeBirthDist>
      <NodeDeathDist>
            <ProbElement>
                 <Type>Exponential</Type>
                 <Weight>0.9</Weight>
                 <Parameter>50</Parameter>
            </ProbElement>
            <ProbElement>
                 <Type>LogNormal</Type>
                 <Weight>1.0</Weight>
                 <Parameter>4.0</Parameter>
                 <Parameter>2.0</Parameter>
            </ProbElement>
      </NodeDeathDist>
      <LinkCreateDist>
            
            <ProbElement>
                 <Type>PoissonPlus</Type>
                 <Weight>1.0</Weight>
                 <Parameter>1.5</Parameter>
                 <Parameter>1.0</Parameter>
            </ProbElement>
      </LinkCreateDist>
      <Parameters>
          <PreferentialAttachment>true</PreferentialAttachment>
      </Parameters>
</ProbabilisticEngine>
