<SimOptions>
  <GlobalController>
     <Port>20001</Port>
     <ConnectedNetwork>false</ConnectedNetwork>
     <Simulation>false</Simulation>
  
   <VisualizationClass>usr.globalcontroller.visualization.ColouredNetworkAndApplicationVisualization</VisualizationClass>
     <Monitoring>
       <LatticeMonitoring>true</LatticeMonitoring>
       <Consumer>
         <Name>usr.globalcontroller.NetIFStatsReporter</Name>
       </Consumer>
       <Consumer>
         <Name>usr.globalcontroller.RouterAppsReporter</Name>
       </Consumer>
     </Monitoring>
  </GlobalController>
    <LocalController>
     <Name>ethane</Name>
     <Port>21000</Port>
     <LowPort>21001</LowPort>
     <HighPort>21999</HighPort>
     <MaxRouters>75</MaxRouters>
  </LocalController>
    <LocalController>
     <Name>methane</Name>
     <Port>22000</Port>
     <LowPort>22001</LowPort>
     <HighPort>22999</HighPort>
     <MaxRouters>75</MaxRouters>
  </LocalController>  
      <LocalController>
     <Name>butane</Name>
     <Port>23000</Port>
     <LowPort>23001</LowPort>
     <HighPort>23999</HighPort>
     <MaxRouters>75</MaxRouters>
  </LocalController>  
  <EventEngine>
     <Name>Probabilistic</Name>
     <EndTime>3600</EndTime>
     <Parameters>monitoring_scripts/gasbed_probdists_noPA.xml</Parameters>
  </EventEngine>
  <RouterOptions>
      monitoring_scripts/gasbed_routeroptions.xml
  </RouterOptions>
      <Output>
        <File>out.net</File>
        <When>End</When>
        <Type>Network</Type>
    </Output>
    <Output>
        <File>summary.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Summary</Type>
    </Output>
    <Output>
        <File>summary.out</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Summary</Type>
    </Output>
   <Output>
        <File>traffic.agg</File>
        <When>Interval</When>
        <Time>10000</Time>
        <Type>Traffic</Type>
        <Parameter>Aggregate</Parameter>
    </Output>
</SimOptions>
